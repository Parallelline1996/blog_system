<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.blog.domain">

    <class name="Follow" table="follow_up">
    	
        <composite-id name="FollowUpId" class="com.blog.domain.FollowUpId"> 
            <key-property name="fansId" type="java.lang.String">
                <column name="fans_ID" />
            </key-property>
            <key-property name="bloggerId" type="java.lang.String">
                <column name="blogger_ID" />
            </key-property>
        </composite-id>
        
    </class>
    
</hibernate-mapping>
