<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.blog.domain">

    <class name="UpOrDown" table="up_down">
    	
        <composite-id name="UpOrDownId" class="com.blog.domain.UpOrDownId"> 
            <key-property name="userId" type="java.lang.String">
                <column name="user_ID" />
            </key-property>
            <key-property name="blogId" type="java.lang.String">
                <column name="blog_ID" />
            </key-property>
        </composite-id>
        
        <property name="state" type="java.lang.Integer">
            <column name="state" />
        </property>
        
    </class>
    
</hibernate-mapping>
